@model IEnumerable<GrupoBJ.Models.Sucursal>

@if (Model.Count() > 0)
{
    @foreach (var item in Model)
    {
    <tr>
        <td hidden>@Html.DisplayFor(m => item.idSucursal)</td>
        <td hidden class="tSuc-Empresa">@Html.DisplayFor(m => item.Empresa.Nombre)</td>
        <td hidden class="tSuc-Sucursal">@Html.DisplayFor(m => item.Nombre)</td>
        <td hidden class="tSuc-Telefono">@Html.DisplayFor(m => item.Telefono)</td>
        <td hidden class="tSuc-CodigoPostal">@Html.DisplayFor(m => item.codigoPostal)</td>
    </tr>
    }
}
else
{
    <br />
    <p>No hay registros</p>
}




