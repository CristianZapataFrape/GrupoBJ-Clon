@model IEnumerable<GrupoBJ.Models.Proveedorcs>
@if (Model.Count() > 0)
{

@foreach (var item in Model)
{
<tr>
    <td hidden>@Html.DisplayFor(m => item.id_Proveedor)</td>
    <td class="tPro-Razon_Social">@Html.DisplayFor(m => item.Razon_Social)</td>
    <td class="tPro-RFC">@Html.DisplayFor(m => item.RFC)</td>
    <td class="tPro-Pais">@Html.DisplayFor(m => item.Pais)</td>
    <td class="tPro-Cuenta">@Html.DisplayFor(m => item.Cuenta)</td>
    <td class="tPro-Telefono">@Html.DisplayFor(m => item.Telefono)</td>
    <td class="tPro-Correo">@Html.DisplayFor(m => item.Correo)</td>
    <td class="tPro-Tiempo_Entrega">@Html.DisplayFor(m => item.Tiempo_Entrega)</td>
</tr>} }
            else
            {
<br />
                <p>No hay registros</p>}
