@model IEnumerable<GrupoBJ.Models.Proveedorcs>
@if (Model.Count() > 0)
{

@foreach (var item in Model)
{
<tr>
    <td hidden>@Html.DisplayFor(m => item.id_Proveedor)</td>
    <td hidden class="tPro-Razon">@Html.DisplayFor(m => item.Razon_Social)</td>
    <td hidden class="tPro-RFC">@Html.DisplayFor(m => item.RFC)</td>
    <td hidden class="tPro-Cuenta">@Html.DisplayFor(m => item.Cuenta)</td>
    <td hidden class="tPro-Telefono">@Html.DisplayFor(m => item.Telefono)</td>
    <td hidden class="tPro-Correo">@Html.DisplayFor(m => item.Correo)</td>
    <td hidden class="tPro-Entrega">@Html.DisplayFor(m => item.Tiempo_Entrega)</td>
</tr>
} 
}
else
{
    <br />
    <p>No hay registros</p>
}
