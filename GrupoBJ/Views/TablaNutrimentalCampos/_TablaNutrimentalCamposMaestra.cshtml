@model IEnumerable<GrupoBJ.Models.Tabla_Nutrimental_Campos>







@if (Model.Count() > 0)
{

    <label id="idMaestro" hidden></label>
    <div class="table-responsive">
        <table class="table table-bordered table-hover" id="tablaArticulos">
            <thead>
                <tr>
                    <th style="width:10%">Id</th>
                    <th style="width:20%">Nombre</th>
                    <th style="width:15%">Usuario Cr</th>
                    <th style="width:15%">Fecha Cr</th>
                    <th style="width:15%">Usuario Um</th>
                    <th style="width:15%">Fecha Um</th>
                </tr>
            </thead>
            <tbody id="ajaxresult">




                @foreach (var item in Model)
                {
                    <tr>
                        <td>@Html.DisplayFor(m => item.id_Tabla_Nutrimental)</td>
                        <td>@Html.DisplayFor(m => item.Nombre_Tabla_Nutrimental_Campos)</td>
                        <td>@Html.DisplayFor(m => item.fkUsuarioCr)</td>
                        <td>@Html.DisplayFor(m => item.fechaCr)</td>
                        <td>@Html.DisplayFor(m => item.fkUsuarioUm)</td>
                        <td>@Html.DisplayFor(m => item.fechaUm)</td>

                    </tr>
                }
            </tbody>
        </table>
    </div>

}
else
{
    <br />
    <p>No hay registros</p>
}
