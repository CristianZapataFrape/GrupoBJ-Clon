@model IEnumerable<GrupoBJ.Models.Empresa>
@if (Model.Count() > 0)
{
    
    @foreach (var item in Model)
    {
    <tr>
        <td hidden>@Html.DisplayFor(m => item.idEmpresa)</td>
        <td hidden class="tEmp-Nombre">@Html.DisplayFor(m => item.Nombre)</td>
        <td hidden class="tEmp-Razon">@Html.DisplayFor(m => item.razonSocial)</td>
        <td hidden class="tEmp-RFC">@Html.DisplayFor(m => item.RFC)</td>
        <td hidden class="tEmp-Telefono">@Html.DisplayFor(m => item.Telefono)</td>
        <td hidden class="tEmp-CodigoPostal">@Html.DisplayFor(m => item.codigoPostal)</td>
    </tr>
    }
}
else
{
    <br />
    <p>No hay registros</p>
}



